[
  {
    "title": "On Player Move",
    "description": "Called at player is moving",
    "since": "1.5",
    "patterns": ["player move [of] [%string%][,] [%string%][,] [%string%]"],
    "examples": [
      "on player move:",
      "on player move of \"y\":",
      "on player move of \"x\":"
    ]
  },
  {
    "title": "On Block Grow",
    "description": "Called when crop is growing",
    "since": "2.0",
    "patterns": ["[block] grow"],
    "examples": ["on grow:"]
  },
  {
    "title": "On Change Locale",
    "description": "Called when player changes locale (Can't cancel this event)",
    "since": "2.0",
    "patterns": ["locale [change]"],
    "examples": ["on locale:"]
  },
  {
    "title": "On Toggle Glide",
    "description": "Called when entity is gliding",
