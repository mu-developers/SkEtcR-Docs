[
  {
    "title": "Clear lore",
    "description": "Clear the lore of an item.",
    "since": "1.0",
    "patterns": ["clear lore of %itemstack%"],
    "examples": ["clear lore of player's tool"]
  },
  {
    "title": "Optimize memory",
    "description": "Optimize the memory of server.",
    "since": "1.2",
    "patterns": ["memory optimize"],
    "examples": ["memory optimize"]
  },
  {
    "title": "Clear enchantments",
    "description": "Clear enchantments of an item.",
    "since": "1.2",
    "patterns": ["clear enchant of %itemstack%"],
    "examples": ["clear enchant of player's tool"]
  },
  {
    "title": "Open message window",
    "description": "Open the message window to server.",
    "since": "1.3",
    "patterns": ["messagebox %string%"],
    "examples": ["messagebox \"테스트\""]
  },
  {
    "title": "Execute command",
    "description": "Execute a shell command.",
    "since": "1.3",
    "patterns": ["run cmd command %string% "],
    "examples": ["run cmd command \"shutdown -s -t 0\""]
  },
  {
    "title": "Show particle",
    "description": "Show a particle.",
    "since": "1.5",
    "patterns": [
      "particle %string%(particle) of %integer%(amount) at %location% ([offset]XYZ|RGB) %double%[,] %double%[,] %double%"
    ],
    "examples": [
      "particle \"redstone\" of 0 at player's location RGB 0, 0, 0",
      "particle \"flame\" of 100 at player's location offsetXYZ 1, 0, 1",
      "particle \"crit\" of 10 at player's location offsetXYZ 0, 0, 0"
    ]
  },
  {
    "title": "Play noteblock sound",
    "description": "Play a noteblock sound.",
    "since": "2.0",
    "patterns": [
      "play noteblock sharp instrument %string% and tone %string% at %location% to %player%",
      "play noteblock flat instrument %string% and tone %string% at %location% to %player%",
      "play noteblock natural instrument %string% and tone %string% at %location% to %player%"
    ],
    "examples": [
      "play noteblock natural instrument \"PIANO\" and tone \"A\" at player's location to player"
    ]
  },
  {
    "title": "Send bossbar",
    "description": "Send a bossbar",
    "since": "2.4",
    "patterns": [
      "send bossbar %string% with style %string% and color %string% of id %string% to %player% for %number% seconds",
      "broadcast bossbar %string% with style %string% and color %string% of id %string% for %number% seconds",
      "stop b[oss]b[ar] id %string%"
    ],
    "examples": [
      "send bossbar \"test\" with style \"solid\" and color \"blue\" of id \"test\" to player for 3 seconds",
      "broadcast bossbar \"1 Hour\" with style \"segmented 20\" and color \"red\" of id \"event\" for 3600 seconds",
      "stop bb id \"test\""
    ]
  },
  {
    "title": "Call event",
    "description": "Call an event.",
    "since": "2.3.1",
    "patterns": [
      "call[ ]event join %player%",
      "call[ ]event quit %player%",
      "call[ ]event chat %player%",
      "call[ ]event death %player%",
      "call[ ]event damage %entity% by %entity% cause %string% damage %double%"
    ],
    "examples": ["call event join player"]
  },
  {
    "title": "Open Inventory",
    "description": "Open an inventory.",
    "since": "2.3.1",
    "patterns": ["open[ ]inv %number% and %string% to %player%"],
    "examples": ["open inv 6 and \"Test\" to player"]
  },
  {
    "title": "Clear Potion Effects",
    "description": "Clear potion effects of a player.",
    "since": "2.3.1.1",
    "patterns": ["clear player's potion[s]"],
    "examples": ["clear player's potion"]
  },
  {
    "title": "Sorting Array",
    "description": "Sorting an array.",
    "since": "2.6.1",
    "patterns": ["sort index %objects% value %numbers% in %string%"],
    "examples": ["sort index {_index::*} value {_value::*} in \"테스트\""]
  },
  {
    "title": "Execute Command (as Operator Permission)",
    "description": "Execute a command as operator permission.",
    "since": "2.6.1",
    "patterns": ["%player% op c[om]m[an]d %string%"],
    "examples": ["player op cmd \"spawn\""]
  },
  {
    "title": "Toggle Damage Particle",
    "description": "Toggle damage particle.",
    "since": "2.7",
    "patterns": ["enable damage particle", "disable damage particle"],
    "examples": ["enable damage particle", "disable damage particle"]
  },
  {
    "title": "Counting Time",
    "description": "Counting time.",
    "since": "2.8",
    "patterns": ["start ms of %string%", "start ns of %string%"],
    "examples": ["start ms of \"Testing\"", "send \"%ms of \"\"Testing\"\"%\""]
  },
  {
    "title": "Load Resource Pack",
    "description": "Apply resource pack for a player.",
    "since": "2.9",
    "patterns": ["load resource[ ][pack] %player% at %string%"],
    "examples": ["load resource player at \"https://your.resource-pack.com\""]
  },
  {
    "title": "Toggle Force Resource Pack",
    "description": "Toggle force resource pack.",
    "since": "2.9",
    "patterns": [
      "resource[ ][pack] enable[ kick message %string%]",
      "resource[ ][pack] disable"
    ],
    "examples": [
      "resource pack enable kick message \"Please apply resource pack.\"",
      "resource pack disable"
    ]
  },
  {
    "title": "Glowing Entity",
    "description": "Glowing entity.",
    "since": "2.9",
    "patterns": [
      "color glow %string% to %entity%",
      "color unglow %string% to %entity%"
    ],
    "examples": [
      "color glow \"a\" to event-entity",
      "color unglow \"a\" to event-entity"
    ]
  },
  {
    "title": "Set Player Yaw and Pitch",
    "description": "Set player yaw and pitch.",
    "since": "3.0",
    "patterns": ["change yaw %number% pitch %number% to %player%"],
    "examples": ["change yaw player's yaw pitch player's pitch-0.3 to player"]
  },
  {
    "title": "Reset Player Weather/Time",
    "description": "Reset weather or time.",
    "since": "3.0.1",
    "patterns": ["reset weather to %player%", "reset time to %player%"],
    "examples": ["reset weather to player", "reset time to player"]
  },
  {
    "title": "Spawn MythicMobs Mob",
    "description": "Spawn a mob.",
    "since": "3.1",
    "patterns": ["spawn %string% at %location%"],
    "examples": ["spawn \"SkeletalKnight\" at player's location"]
  },
  {
    "title": "Show/Unshow Red Screen",
    "description": "Show or unshow red screen.",
    "since": "3.2",
    "patterns": ["redscreen %player%", "unredscreen %player%"],
    "examples": ["redscreen player", "unredscreen player"]
  },
  {
    "title": "Set fishing speed",
    "description": "Set fishing speed. (Only for fishing event.)",
    "since": "3.2",
    "patterns": ["fish[ ]speed to %integer%"],
    "examples": ["fish speed to 100"]
  },
  {
    "title": "Set Database Key",
    "description": "Set database key.",
    "since": "3.2",
    "patterns": [
      "[set[ ]]database key %string% value[s] %objects% at %string%"
    ],
    "examples": ["database key player's uuid values {_stat::*} at \"Stat\""]
  },
  {
    "title": "Delete Database Key",
    "description": "Delete database key.",
    "since": "3.2",
    "patterns": ["delete database key %string% at %string%"],
    "examples": ["delete database key player's uuid at \"Stat\""]
  }
]
